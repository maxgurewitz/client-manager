{
  "files": [
    "startServer.ts"
  ],
  "filesGlob": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.d.ts"
  ],
  "compilerOptions": {
    "lib": ["ES6", "DOM"],
    "strictNullChecks": true,
    "outDir": "./dist/",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "noImplicitAny": false,
    "sourceMap": true
  }
}
